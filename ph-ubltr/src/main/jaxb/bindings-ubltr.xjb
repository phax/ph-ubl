<bindings version="3.0" 
          xmlns="https://jakarta.ee/xml/ns/jaxb"
          xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
          xmlns:xsd="http://www.w3.org/2001/XMLSchema"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
          xsi:schemaLocation="https://jakarta.ee/xml/ns/jaxb https://jakarta.ee/xml/ns/jaxb/bindingschema_3_0.xsd">
  <globalBindings typesafeEnumMaxMembers="2000" typesafeEnumMemberName="generateError">
    <xjc:javaType name="com.helger.datetime.xml.XMLOffsetDateTime" xmlType="xsd:dateTime" adapter="com.helger.jaxb.adapter.AdapterXMLOffsetDateTime" />
    <xjc:javaType name="com.helger.datetime.xml.XMLOffsetDate"     xmlType="xsd:date"     adapter="com.helger.jaxb.adapter.AdapterXMLOffsetDate" />
    <xjc:javaType name="com.helger.datetime.xml.XMLOffsetTime"     xmlType="xsd:time"     adapter="com.helger.jaxb.adapter.AdapterXMLOffsetTime" />
  </globalBindings>
  <bindings schemaLocation="../resources/external/schemas/ubltr/Envelope/Package_1_2.xsd" node="/xsd:schema">
    <schemaBindings>
      <package name="tr.gov.efatura.package_12" />
      <!-- Required because "Package" is reserved class name -->
      <nameXmlTransform>
        <typeName prefix="TR" />
        <elementName prefix="TR" />
      </nameXmlTransform>
    </schemaBindings>
  </bindings>
  <bindings schemaLocation="../resources/external/schemas/ubltr/HRXML/UserAccount.xsd" node="/xsd:schema">
    <schemaBindings>
      <package name="tr.gov.efatura.useraccount" />
    </schemaBindings>
  </bindings>
  <bindings schemaLocation="../resources/external/schemas/ubltr/Userlist/NewUserList.xsd" node="/xsd:schema">
    <schemaBindings>
      <package name="tr.gov.efatura.userlist" />
      <nameXmlTransform>
        <typeName prefix="UL"/>
      </nameXmlTransform>
    </schemaBindings>
  </bindings>
</bindings>
